<Window
    x:Class="E2P.Views.ExcelFileWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="using:E2P.Views"
    xmlns:vm="clr-namespace:E2P.ViewModels"
    Title="Excel File"
    Width="800"
    Height="600"
    x:DataType="vm:ExcelFileViewModel"
    WindowStartupLocation="CenterOwner"
    mc:Ignorable="d">
    <StackPanel>
        <TextBlock
            Margin="10"
            FontSize="16"
            FontWeight="Bold"
            Text="Selected Excel File" />
        <TextBlock Margin="10" Text="{Binding FilePath}" />
        <!--  Here you could add a DataGrid or other UI components to display Excel file content  -->
    </StackPanel>
</Window>